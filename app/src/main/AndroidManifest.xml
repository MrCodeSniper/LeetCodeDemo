                <data android:scheme="http" />
            </intent-filter>
        </activity> <!-- 推送相关 start -->
        <service
            android:name="com.alipay.pushsdk.push.NotificationService"
            android:enabled="true"
            android:exported="false"
            android:label="NotificationService"
            android:process=":push">
